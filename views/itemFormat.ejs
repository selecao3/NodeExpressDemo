<!DOCTYPE html>

<%- include('./parts/htmlSetting') %>

<body>
    <%- include('./parts/header') %>
    <!--folderNameからContentBoxに変更しておく-->
    <div id="main-edit">
        <% items.forEach(function (v,k) { %>
        <div class="folderName">
            <table style="width:100%;font-size: 20px">
                <tbody>
                    <tr>
                        <td>
                            <b>内容</b>
                            <div>
                                <%= v.checkFin %>
                                <%= v.questionsCon %>
                            </div>
                            <!-- pagingと検索 -->
                            <hr>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <b>ユーザー情報</b>
                            <table style="width:100%;">
                                <tbody>
                                    <tr>
                                        <!-- "身分"とかを協調 -->
                                        <td colspan="2">身分：
                                            <%= v.ident%>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>氏名：
                                            <%= v.questioner%>
                                        </td>
                                        <td>学生番号：
                                            <%= v.questionersNumber%>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>連絡先：
                                            <%= v.questionersAddress%>
                                        </td>
                                        <td>基盤ID：
                                            <%= v.questionersID%>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <hr>
                        </td>
                    </tr>

                    <tr>
                        <td>
                            <b>特記事項</b>
                            <div>
                                <%= v.specialText %>
                            </div>
                            <hr>
                        </td>
                    </tr>
                    <tr style="text-align:right;">
                        <td>対応日時：
                            <%= v.date %>
                            <br>担当：
                            <%= v.stuff %>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <a href="/data2csv/?id=<%= v.id%>">aaa</a>
        <% }); %>
    </div>




</body>

</html>
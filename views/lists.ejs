<!DOCTYPE html>
<html>



<%- include('./htmlSetting') %>

<body>
    <%- include('./header') %>


    <div id="main">
        <!-- Modal HTML embedded directly into document -->

        <div id="addFolder" class="modal">
            <form action="/post/folder" method="post">
                <p>
                    追加するフォルダ名:
                    <input type="text" name="folderName" style="width:250px;">
                </p>
                <input type="submit" value="OK">
            </form>
            <a href="#" rel="modal:close">Close</a>
        </div>
        <div id="home">
            <div id="main-menu">
                <% var filename = [['Office365','id','title'],['その他','id2','title2']] %>
                <% filename.forEach(function(v,k) { %>
                <div class="folderName">
                    <span id="title">
                        <a href="/lists">
                            <a href="/lists/<%= v[0] %>">
                            <%= v[0] %>
                            </a>
                        </a>
                    </span>
                    <hr>
                    <p>更新日時：2018年12月21日
                    </p>
                </div>
                <% }); %>
            </div>
            <!-- </div> -->
        </div>

    </div>


</body>

</html>